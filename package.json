{
    "name": "pluto-vscode",
    "description": "Pluto inside VS Code",
    "version": "0.0.1",
    "publisher": "juliacomputing",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/JuliaComputing/pluto-vscode"
    },
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:plutoView.start",
        "onWebviewPanel:plutoView"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "plutoView.start",
                "title": "Start new notebook",
                "category": "Pluto"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -w -p ./"
    },
    "devDependencies": {
        "@types/node": "^12.19.3",
        "@types/vscode": "^1.47.0",
        "@types/vscode-webview": "^1.57.0",
        "@typescript-eslint/eslint-plugin": "^4.16.0",
        "@typescript-eslint/parser": "^4.16.0",
        "eslint": "^7.21.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "@types/ws": "^7.4.7",
        "ws": "^8.2.2"
    }
}
